
<table class="table table-bordered">

    <thead>
        <tr>
            <th width="10">Number</th>
            <th>Title</th>
            <th>Writer</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var note in Model)
        {
            <tr>
                <td>@note.No</td>
                <td>
                    <!-- asp-route-{variable name} : [GET] send parameta to controller -->
                    <a asp-controller="Note" asp-action="Detail" asp-route-noteNo="@note.No">@note.Title</a>
                </td>
                <td>@note.UserNo</td>
            </tr>
        }
    </tbody>

</table>

<a class="btn btn-warning" asp-controller="Note" asp-action="Add">Write</a>